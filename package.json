{
  "name": "n8n-nodes-filemanager",
  "version": "0.2.3",
  "description": "Manage files and folders on disk with create, copy, move, remove, and rename operations for n8n workflows",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes-filemanager",
    "n8n-nodes",
    "file",
    "folder",
    "filesystem",
    "copy",
    "move",
    "remove",
    "create",
    "rename"
  ],
  "license": "MIT",
  "homepage": "https://github.com/xopoko/n8n-nodes-filemanager#readme",
  "author": {
    "name": "horoko",
    "email": "cyprus.horoko@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xopoko/n8n-nodes-filemanager.git"
  },
  "bugs": {
    "url": "https://github.com/xopoko/n8n-nodes-filemanager/issues"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/FileManager/FileManager.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^5.0.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.82.0"
  }
}
